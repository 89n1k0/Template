<Window x:Class="Template_4333._4333_Fedyanin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4333"
        mc:Ignorable="d"
        Title="_4333_Fedyanin" Height="450" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="Федянин Артур Романович, группа 4333, 18 лет" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="20" TextAlignment="Center"/>
        <Button Grid.Row="1" Content="Импорт данных в бд" Width="300" Height="50" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Background="Black" Foreground="White" Click="import_Click"/>
        <Button Grid.Row="2" Content="Экспорт данных в Excel" Width="300" Height="50" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Background="Black" Foreground="White" Click="export_Click"/>
    </Grid>
</Window>
